# -----------------------------------------------------
# TEST 2: RPN Calculator
# -----------------------------------------------------
# Expression: "9,3-"
# Expected result: 6
# -----------------------------------------------------

.section .data

exp:
    .string "9,3-"

res:
    .int -1                # output result placeholder

.section .text

    # After ex2 returns, check if res matches expected value
    movl res(%rip), %eax
    cmp $6, %eax
    je success

error:
    mov $60, %rax              # sys_exit
    mov $1, %rdi               # exit code 1 (error)
    syscall

success:
    mov $60, %rax              # sys_exit
    xor %rdi, %rdi             # exit code 0 (success)
    syscall
