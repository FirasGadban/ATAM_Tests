# -----------------------------------------------------
# TEST 13: RPN Calculator
# -----------------------------------------------------
# Expression: "1,2+3*"
# Expected result: 9
# -----------------------------------------------------

.section .data

exp:
    .string "1,2+3*"

res:
    .int -1                # output result placeholder

.section .text

    # After ex2 returns, check if res matches expected value
    movl res(%rip), %eax
    cmp $9, %eax
    je success

error:
    mov $60, %rax              # sys_exit
    mov $1, %rdi               # exit code 1 (error)
    syscall

success:
    mov $60, %rax              # sys_exit
    xor %rdi, %rdi             # exit code 0 (success)
    syscall
